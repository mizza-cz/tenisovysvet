{% for fontName in ["GeneralSans-Regular", "GeneralSans-Medium", "GeneralSans-Semibold", "GeneralSans-Bold"] %}
    <link rel="preload" href="./fonts/{{ fontName }}.woff" as="font" type="font/woff2" crossorigin="anonymous"/>
    <link rel="preload" href="./fonts/{{ fontName }}.woff2" as="font" type="font/woff" crossorigin="anonymous"/>
{% endfor %}

<style>
    @font-face {
        font-family: "General Sans";
        src: url("./fonts/GeneralSans-Regular.woff2") format("woff2"), url("./fonts/GeneralSans-Regular.woff") format("woff");
        font-style: normal;
        font-weight: 400;
    }

    @font-face {
        font-family: "General Sans";
        src: url("./fonts/GeneralSans-Medium.woff2") format("woff2"), url("./fonts/GeneralSans-Medium.woff") format("woff");
        font-style: normal;
        font-weight: 500;
    }

    @font-face {
        font-family: "General Sans";
        src: url("./fonts/GeneralSans-Semibold.woff2") format("woff2"), url("./fonts/GeneralSans-Semibold.woff") format("woff");
        font-style: normal;
        font-weight: 600;
    }

    @font-face {
        font-family: "General Sans";
        src: url("./fonts/GeneralSans-Bold.woff2") format("woff2"), url("./fonts/GeneralSans-Bold.woff") format("woff");
        font-style: normal;
        font-weight: 700;
    }
</style>
